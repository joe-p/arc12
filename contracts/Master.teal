#pragma version 8
	b main

abi_route_create:
	txn ApplicationID
	int 0
	==
	assert
	callsub create
	int 1
	return

create:
	proto 0 0

	// arc12.ts:129
	// assert(this.txn.applicationID === new Application(0))
	txn ApplicationID
	int 0
	==
	assert
	retsub

abi_route_createVault:
	txn OnCompletion
	int NoOp
	==
	assert
	txna ApplicationArgs 1
	btoi
	txnas Accounts
	txn GroupIndex
	int 1
	-
	callsub createVault
	int 1
	return

createVault:
	proto 2 0

	// arc12.ts:133
	// assert(!this.vaultMap.exists(receiver))
	frame_dig -2 // receiver: Account
	box_get
	swap
	pop
	!
	assert

	// arc12.ts:134
	// assert(mbrPayment.receiver === globals.currentApplicationAddress)
	frame_dig -1 // mbrPayment: PayTxn
	gtxns Receiver
	global CurrentApplicationAddress
	==
	assert

	// arc12.ts:135
	// assert(mbrPayment.sender === this.txn.sender)
	frame_dig -1 // mbrPayment: PayTxn
	gtxns Sender
	txn Sender
	==
	assert

	// arc12.ts:136
	// assert(mbrPayment.closeRemainderTo === globals.zeroAddress)
	frame_dig -1 // mbrPayment: PayTxn
	gtxns CloseRemainderTo
	global ZeroAddress
	==
	assert

	// arc12.ts:138
	// preCreateMBR = globals.currentApplicationAddress.minBalance
	global CurrentApplicationAddress
	acct_params_get AcctMinBalance
	assert
	store 0 // preCreateMBR: uint64

	// arc12.ts:140
	// sendMethodCall<[Account, Account], void>({
	itxn_begin
	int appl
	itxn_field TypeEnum
	method "create(account,account)void"
	itxn_field ApplicationArgs

	// arc12.ts:142
	// OnCompletion: 'NoOp'
	int NoOp
	itxn_field OnCompletion

	// arc12.ts:143
	// fee: 0
	int 0
	itxn_field Fee

	// arc12.ts:144
	// methodArgs: [receiver, this.txn.sender]
	frame_dig -2 // receiver: Account
	itxn_field Accounts
	int 1
	itob
	itxn_field ApplicationArgs
	txn Sender
	itxn_field Accounts
	int 2
	itob
	itxn_field ApplicationArgs

	// arc12.ts:145
	// clearStateProgram: this.app.clearStateProgram
	txna Applications 0
	app_params_get AppClearStateProgram
	assert
	itxn_field ClearStateProgram

	// arc12.ts:146
	// approvalProgram: Vault
	byte b64 
	itxn_field ApprovalProgram

	// arc12.ts:147
	// globalNumByteSlice: 2
	int 2
	itxn_field GlobalNumByteSlice

	// arc12.ts:148
	// globalNumUint: 1
	int 1
	itxn_field GlobalNumUint
	itxn_submit

	// arc12.ts:151
	// vault = this.itxn.createdApplicationID
	itxn CreatedApplicationID
	store 1 // vault: Application

	// arc12.ts:153
	// sendPayment({
	itxn_begin
	int pay
	itxn_field TypeEnum

	// arc12.ts:154
	// receiver: vault.address
	load 1 // vault: Application
	app_params_get AppAddress
	assert
	itxn_field Receiver

	// arc12.ts:155
	// amount: globals.minBalance
	global MinBalance
	itxn_field Amount

	// arc12.ts:156
	// fee: 0
	int 0
	itxn_field Fee
	itxn_submit

	// arc12.ts:159
	// this.vaultMap.put(receiver, vault)
	frame_dig -2 // receiver: Account
	load 1 // vault: Application
	itob
	box_put

	// arc12.ts:162
	// assert(mbrPayment.amount === (globals.currentApplicationAddress.minBalance - preCreateMBR) + globals.minBalance)
	frame_dig -1 // mbrPayment: PayTxn
	gtxns Amount
	global CurrentApplicationAddress
	acct_params_get AcctMinBalance
	assert
	load 0 // preCreateMBR: uint64
	-
	global MinBalance
	+
	==
	assert

	// arc12.ts:164
	// return vault;
	load 1 // vault: Application
	itob
	byte 0x151f7c75
	swap
	concat
	log
	retsub

abi_route_verifyAxfer:
	txn OnCompletion
	int NoOp
	==
	assert
	txna ApplicationArgs 1
	btoi
	txnas Accounts
	txn GroupIndex
	int 1
	-
	txna ApplicationArgs 2
	btoi
	txnas Applications
	callsub verifyAxfer
	int 1
	return

verifyAxfer:
	proto 3 0

	// arc12.ts:168
	// assert(this.vaultMap.exists(receiver))
	frame_dig -3 // receiver: Account
	box_get
	swap
	pop
	assert

	// arc12.ts:170
	// assert(this.vaultMap.get(receiver) === vault)
	frame_dig -3 // receiver: Account
	box_get
	assert
	btoi
	frame_dig -1 // vault: Application
	==
	assert

	// arc12.ts:171
	// assert(vaultAxfer.assetReceiver === vault.address)
	frame_dig -2 // vaultAxfer: AssetTransferTxn
	gtxns AssetReceiver
	frame_dig -1 // vault: Application
	app_params_get AppAddress
	assert
	==
	assert

	// arc12.ts:172
	// assert(vaultAxfer.assetCloseTo === globals.zeroAddress)
	frame_dig -2 // vaultAxfer: AssetTransferTxn
	gtxns AssetCloseTo
	global ZeroAddress
	==
	assert
	retsub

